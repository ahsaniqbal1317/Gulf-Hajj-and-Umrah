@model Gulf_Hajj_and_Ummrah.Models.airline_tbl

@{
    ViewBag.Title = "AddOrEditAirline_PartialView";
}


@using (Html.BeginForm("AddOrEditAirline", "Administration", FormMethod.Post))
{
    @Html.HiddenFor(model => model.id)
    <div class="form-group">
        <label>Roomtype</label>
        @Html.EditorFor(model => model.airlineName, new { htmlAttributes = new { @class = "form-control", @autocomplete = "off", @placeholder = "Enter Airline" } })
        @Html.ValidationMessageFor(model => model.airlineName, "", new { @class = "text-danger" })
    </div>
    <div class="form-group">
        <input type="submit" value="Submit" class="btn btn-primary" />
        <input type="reset" value="Reset" class="btn btn-warning" />
    </div>
}
<script src="~/Scripts/jquery.validate.min.js"></script>
<script src="~/Scripts/jquery.validate.unobtrusive.min.js"></script>

